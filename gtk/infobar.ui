<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="Infobar" parent="GtkWidget">
    <child>
      <object class="GtkActionBar" id="action_bar">
        <property name="revealed">false</property>

        <child type="start">
          <object class="GtkLabel">
            <property name="label">(</property>
          </object>
        </child>

        <child type="start">
          <object class="GtkLabel" id="x">
            <property name="label">0</property>
          </object>
        </child>

        <child type="start">
          <object class="GtkLabel">
            <property name="label">,</property>
          </object>
        </child>

        <child type="start">
          <object class="GtkLabel" id="y">
            <property name="label">0</property>
          </object>
        </child>

        <child type="start">
          <object class="GtkLabel">
            <property name="label">)</property>
          </object>
        </child>

        <child type="start">
          <object class="GtkLabel" id="values">
            <property name="label">    0     0     0</property>
          </object>
        </child>

        <child type="end">
          <object class="GtkLabel" id="mag">
            <property name="label">Magnification 1:1</property>
          </object>
        </child>

      </object>
    </child>
  </template>
</interface>
